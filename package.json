{
  "name": "tour-planner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:fix": "eslint src --ext js,jsx,ts,tsx --report-unused-disable-directives --fix --max-warnings 23",
    "lint:format": "prettier --loglevel warn --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc.cjs",
    "lint": "npm run lint:format && npm run lint:fix",
    "server": "npx json-server --watch db.json --port 3004"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@tanstack/react-query": "^4.32.6",
    "@tanstack/react-table": "^8.9.3",
    "@wojtekmaj/react-daterange-picker": "^5.4.2",
    "@xeger/quill-image-actions": "^0.7.1",
    "@xeger/quill-image-formats": "^0.7.1",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "jwt-decode": "^3.1.2",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.45.4",
    "react-quill": "^2.0.0",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "react-use": "^17.4.0",
    "sanitize-html": "^2.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@tailwindcss/forms": "^0.5.4",
    "@types/quill": "^2.0.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/sanitize-html": "^2.9.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "json-server": "^0.17.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier-eslint": "^15.0.1",
    "sass": "^1.65.1",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "npx prettier --write",
      "npx eslint --ext js,jsx,ts,tsx",
      "git add"
    ]
  }
}
